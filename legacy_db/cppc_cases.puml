		ADDRESS_2 varchar(50)
		ADDRESS_3 varchar(50)
		ADDRESS_4 varchar(50)
		POSTCODE : varchar(20)
		PHONE : varchar(20)
		FAX : varchar(20)
		EMAIL : varchar(50)
