	class SFO_ADDITIONAL_OFFENCE <<JoinTable>> {
		* ID : int
		-- Foreign Keys
		* SFO : int
		* OFFENCE : int
	}

	class SFO_ANSWER <<LookupTable>> {
		* ID : int
		-- Foreign Keys --
		---
		* Description : varchar(50)
		-- Visibility --
		SHOW_TO_UNIT : int
		-- Audit --
		DELETED : datetime
		LOV_DELETED_BY : int
		LOV_UPDATED_BY : int
		LOV_UPDATED_DATE : datetime
		LOV_CREATED_BY : int
		LOV_CREATED_DATE : datetime
	}

	class SFO_CRIM_FACTOR_RECONVICTION <<JoinTable>> {
		* ID : int
		-- Foreign Keys --
		* SFO : int
		* CRIM_FACTOR : int
		---
	}

	class SFO_CRIM_FACTOR_SERIOUS_HARM <<JoinTable>> {
		* ID : int
		-- Foreign Keys --
		* SFO : int
		* CRIM_FACTOR : int
		---
	}

	class SFO_MAPPA_CAT <<LookupTable>> {
		* ID : int
		-- Foreign Keys --
		---
		* DESCRIPTION : varchar(50)
		-- Visibility --
		SHOW_TO_UNIT : int
		-- Audit --
		DELETED : datetime
		LOV_DELETED_BY : int
		LOV_UPDATED_BY : int
		LOV_UPDATED_DATE : datetime
		LOV_CREATED_BY : int
		LOV_CREATED_DATE : datetime
	}

	class SFO_PROBATION_AREA <<JoinTable>> {
		* ID : int
		* SFO : int
		* PROBATION_AREA : int
	}

	class SFO_SHORT_STAGE <<LookupTable>> {
		* ID : int
		-- Foreign Keys --
		---
		* DESCRIPTION : varchar(50)
		-- Visibility --
		SHOW_TO_UNIT : int
		-- Audit --
		DELETED : datetime
		LOV_DELETED_BY : int
		LOV_UPDATED_BY : int
		LOV_UPDATED_DATE : datetime
		LOV_CREATED_BY : int
		LOV_CREATED_DATE : datetime
	}

	class SFO_STAGE <<LookupTable>> {
		* ID : int
		-- Foreign Keys --
		---
		* Description : varchar(50)
		* SHORT : bit
		-- Visibility --
		SHOW_TO_UNIT : int
		-- Audit --
		DELETED : datetime
		LOV_DELETED_BY : int
		LOV_UPDATED_BY : int
		LOV_UPDATED_DATE : datetime
		LOV_CREATED_BY : int
		LOV_CREATED_DATE : datetime
	}

	class SFO_STATUS <<LookupTable>> {
