		LOV_UPDATED_BY : int
		LOV_UPDATED_DATE : datetime
		LOV_CREATED_BY : int
		LOV_CREATED_DATE : datetime
	}

	class SFO_SUPERVISION_REQUIREMENT {
		* ID : int
		-- Foreign Keys --
